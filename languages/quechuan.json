    "specialRules": {
        "quechuaOrthography": {
            "description": "Modern Quechua uses Latin script with special characters",
            "specialCharacters": ["ñ", "ch", "ch'", "k", "k'", "kh", "ll", "p", "p'", "ph", "q", "q'", "qh", "t", "t'", "th"],
            "example": "Q'orianka, P'asña, Ch'aska"
        },
        "incaNaming": {
            "description": "Inca rulers often took new names upon ascending",
            "example": "Birth name: Cusi Yupanqui → Ruler name: Pachacútec"
        },
        "spanishInfluence": {
            "description": "Many Quechua speakers have Spanish first names with Quechua surnames",
            "example": "José Quispe, María Condori"
        },
        "doubleNaming": {
            "description": "Some use both Quechua and Spanish names",
            "example": "Tupaq José, Inti Carlos"
        },
        "genderNeutralNames": {
            "description": "Many Quechua names are unisex",
            "example": "Inti (sun), Killa (moon), Qoyllur (star)"
        },
        "compoundNames": {
            "description": "Names can be combined for meaning",
            "example": "Intiraymi (sun festival), Pachamama (earth mother)"
        },
        "sacredNames": {
            "description": "Some names invoke Andean deities",
            "example": "Inti (sun god), Illapa (thunder god), Mama Killa (moon goddess)"
        },
        "animalNames": {
            "description": "Many names reference Andean animals",
            "example": "Kuntur (condor), Puma (puma), Amaru (serpent)"
        },
        "placeNames": {
            "description": "Some surnames derive from place names",
            "example": "Cusco, Ayacucho, Huancayo"
        }
    }
