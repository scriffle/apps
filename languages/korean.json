        "syllableStructure": {
            "description": "Most Korean given names are two syllables",
            "example": "Min-ho, Ji-woo, Sung-jin"
        },
        "hanjaOrigin": {
            "description": "Names often have Chinese character (Hanja) meanings",
            "example": "Min (敏) = clever, Ho (浩) = great"
        },
        "generationNames": {
            "description": "Siblings may share one syllable of their given name",
