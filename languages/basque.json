    "specialRules": {
        "orthography": {
            "description": "Basque uses specific letter combinations",
            "combinations": ["tx", "tz", "ts", "tt", "dd", "rr", "ll", "ñ"],
            "pronunciation": {
                "tx": "ch as in church",
                "tz": "ts as in cats",
                "ts": "ts softer",
                "x": "sh",
                "j": "y or j",
                "ñ": "ny"
            }
        },
        "wordFormation": {
            "description": "Basque names often combine nature elements",
            "examples": {
                "Haizea": "haize (wind) + a",
                "Itsaso": "itsas (sea) + o",
                "Mendizabal": "mendi (mountain) + zabal (wide)"
            }
        },
        "genderNeutrality": {
            "description": "Many Basque names are gender-neutral",
            "examples": ["Amets", "Zorion", "Haize", "Eder"]
        },
        "locativeNames": {
            "description": "Many surnames indicate location",
            "suffixes": {
                "etxe": "house",
