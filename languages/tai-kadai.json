    "specialRules": {
        "thaiNicknames": {
            "description": "Thais use nicknames (chue len) from birth, often unrelated to formal names",
            "examples": ["Nok (bird)", "Lek (small)", "Daeng (red)", "Noi (little)", "Fon (rain)", "Nam (water)"]
        },
        "laoHonorifics": {
            "male": ["Thao", "Aai", "Loung", "Thit"],
            "female": ["Nang", "Sao", "Mae", "Daeng"],
            "description": "Age and status markers used before names"
        },
        "toneMarks": {
            "description": "Tai-Kadai languages are tonal; same spelling can have different meanings",
            "thai": ["่", "้", "๊", "๋"],
            "lao": ["່", "້", "໊", "໋"]
        },
        "royalVocabulary": {
            "description": "Special vocabulary used for royalty (rachasap)",
            "examples": {
                "eat": "sawoey (royal) vs kin (common)",
                "sleep": "banthom (royal) vs norn (common)",
                "speak": "tras (royal) vs phood (common)"
            }
        },
        "buddhist_influence": {
            "description": "Many names have Sanskrit/Pali origins due to Buddhism",
            "examples": ["Siri (glory)", "Mani (gem)", "Ratana (jewel)", "Dhamma (dharma)", "Sangha (community)"]
        },
        "generationNaming": {
            "description": "Some families use related names for siblings",
            "example": "All daughters might have 'Noi' (little) in their name"
        },
        "spiritNames": {
            "description": "Unflattering nicknames to confuse evil spirits",
            "examples": ["Ouan (fat)", "Dum (black)", "Ee (short)", "Gob (frog)"]
        },
        "muang": {
            "description": "Traditional city-state naming system",
            "examples": ["Muang Luang Prabang", "Muang Vientiane", "Muang Chiang Mai", "Muang Sukhothai"]
        }
    }
